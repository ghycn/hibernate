<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.org.ghy.bean.UserEntity" table="user" schema="test">
        <id name="id">
            <column name="ID" sql-type="varchar(36)" length="36"/>
        </id>
        <property name="name">
            <column name="NAME" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="password">
            <column name="PASSWORD" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="age">
            <column name="AGE" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="sex">
            <column name="SEX" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="birthday">
            <column name="BIRTHDAY" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>